{
  "version": 2,
  "builds": [
    {
      "src": "server/flask-server.py",
      "use": "@vercel/python"
    },
    {
      "src": "build",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/upload", "dest": "/server/flask-server.py" },
    { "src": "/api/(.*)", "dest": "/server/flask-server.py" },
    { "src": "/(.*)", "dest": "/build/$1" }
  ]
}
